{
  "name": "PCLN Random Search",
  "description": "Chrome extension to generate random searches on Priceline",
  "version": "2.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "favicon-16x16.png",
      "38": "favicon-32x32.png"
    }
  },
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
